<Window x:Class="SelSovetApp.Windows.HtmlClientReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SelSovetApp.Windows"
        mc:Ignorable="d"
        Title="Отчет по клиентам" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <WebBrowser x:Name="WBrowserOrder"/>
        <Button Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Bottom" Click="Button_Click">Сохранить</Button>
    </Grid>
</Window>
